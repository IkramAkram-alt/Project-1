<%= form_for @contact do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :name %><br>
  <%= f.label :address %>
  <%= f.text_area :address %><br>
  <%= f.label :phone %>
  <%= f.text_field :phone %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.submit 'Contact added' %>
<% end %>
